template template_FOUNDATIONS_x1(nr,fn, y1,h, rx,ry) {
	// nr    position in spritesheet
	// fn    filename
	// y1    top offset of child
	// h     height of sprite
	// rx,ry position offset
	[2560 + (nr % 10)* 64, (nr / 10)* 80 + y1,  64,   h,   rx,   ry, NOCROP | NOALPHA, fn]
}

template template_FOUNDATIONS_x4(nr,fn, y1,h, rx,ry) {
	//x for movement to next tile
	//y for Y offsets
	[       (nr % 10)*256, (nr / 10)*320 + y1, 256, 4*h, 4*rx, 4*ry, NOCROP | NOALPHA, fn]
}


template template_FOUNDATIONS_x1_flat(nr,fn) {
	template_FOUNDATIONS_x1(nr+ 0, fn, 0, 40, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 1, fn, 0, 32, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 2, fn, 0, 32, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 3, fn, 0, 40, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 4, fn, 0, 40, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 5, fn, 0, 32, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 6, fn, 0, 32, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 7, fn, 0, 40, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 8, fn, 0, 40, -31,  -9)
	template_FOUNDATIONS_x1(nr+ 9, fn, 0, 32, -31,  -9)
	template_FOUNDATIONS_x1(nr+10, fn, 0, 32, -31,  -9)
	template_FOUNDATIONS_x1(nr+11, fn, 0, 40, -31,  -9)
	template_FOUNDATIONS_x1(nr+12, fn, 0, 40, -31,  -9)
	template_FOUNDATIONS_x1(nr+13, fn, 0, 32, -31,  -9)
}

